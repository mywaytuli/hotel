<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.ahut.login.entity">

	<class name="User" table="users">
		<id name="id" column="id" type="java.lang.String" length="32">
			<generator class="uuid.hex"></generator>
		</id>
		<property name="userName" type="java.lang.String"></property>
		<property name="account" type="java.lang.String"></property>
		<property name="role" type="java.lang.String"></property>
		<property name="mobile" type="java.lang.String"></property>
		<property name="pwd" type="java.lang.String"></property>
		<property name="address" type="java.lang.String"></property>
	</class>

</hibernate-mapping>



